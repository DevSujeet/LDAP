# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ThirdPartySignin' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for googleSignin
pod 'GoogleSignIn'
  # Pods for MicrosoftSignin
pod 'MSAL'
  # Pods for pingFederate- following https://github.com/pingidentity/ios-appauth-sample-application
  pod 'AppAuth'
end
